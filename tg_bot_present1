        private static async Task<string> anniversaryAsync(ITelegramBotClient botClient, Message message)
        {
            await botClient.SendTextMessageAsync(message.Chat.Id, // –ù–∞–ø–∏—à–µ—Ç –≤ —á–∞—Ç 
