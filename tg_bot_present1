        private static async Task Main(string[] args)
        {
            Console.OutputEncoding = System.Text.Encoding.UTF8;
            //var botClient = new TelegramBotClient("5671400176:AAFVG9763O-Tq1YZM_5CEx6P12v3DL72qX0");
            //–ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¢–ì –±–æ—Ç–∞
            using var cts = new CancellationTokenSource();
            //–ù–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–æ–ª—É—á–µ–Ω–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π
            var receiverOptions = new ReceiverOptions
            {
                AllowedUpdates = { }
            };
            //–§—É–Ω–∫—Ü–∏—è –≥–¥–µ –º—ã —ç—Ç–æ –∫–æ–º–ø–æ–Ω—É–µ–º –∏ –ø–æ–ª—É—á–∞–µ–º –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –æ—Ç –¢–µ–ª–µ–≥—Ä–∞–º
            botClient.StartReceiving(
                HandleUpdatesAsync,//–º–µ—Ç–æ–¥, –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π –±–æ—Ç–∞
                HandleErrorAsync,//–º–µ—Ç–æ–¥ –æ–±—Ä–∞–±–æ—Ç–∫–∏ –æ—à–∏–±–æ–∫
                receiverOptions,//–Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –ø–æ–ª—É—á–µ–Ω–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏–π
                cancellationToken: cts.Token);//—Ç–æ–∫–µ–Ω –æ—Ç–º–µ–Ω—ã

            var me = await botClient.GetMeAsync();

            Console.WriteLine($"–ù–∞—á–∞–ª —Ä–∞–±–æ—Ç—É c @{me.Username}");

            Console.ReadLine();

            cts.Cancel();//–æ—Ç–º–µ–Ω–∞ —Ç–æ–∫–µ–Ω–∞
                         //–æ–±—Ä–∞–±–æ—Ç–∫–∞ —Å–æ–±—ã—Ç–∏–π

            async Task HandleUpdatesAsync(ITelegramBotClient botClient, Update update, CancellationToken cancellationToken)
            {
                if (update.Type == UpdateType.Message && update?.Message?.Text != null)
                {
                    await HandleMessage(botClient, update.Message);
                    return;
                }

                /* if (update.Type == UpdateType.CallbackQuery)
                 {
                     await HandleCallbackQuery(botClient, update.CallbackQuery);
                     return;
                 }*/
            }
            
            Task HandleErrorAsync(ITelegramBotClient botClient, Exception exception, CancellationToken cancellationToken)
            {
                var ErrorMessage = exception switch
                {
                    ApiRequestException apiRequestException
                        => $"Telegram API Error:\n[{apiRequestException.ErrorCode}]\n{apiRequestException.Message}",
                    _ => exception.ToString()
                };

                Console.WriteLine(ErrorMessage);
                return Task.CompletedTask;
            }

            async Task HandleMessage(ITelegramBotClient botClient, Message message)
            {
                //id –≤ –ø–µ—Ä–µ—Å—ã–ª–∞–µ–º—ã–π —á–∞—Ç
                
                // –º–µ–Ω—é –ü–†–°
                string[] seminarRequest = new string[3];
                //–≥–ª–∞–≤–Ω–∞—è —Å—Ç—Ä–∞–Ω–∏—Ü–∞
                if (message.Text == "/start" || message.Text == "ü™ú–ù–∞ –≥–ª–∞–≤–Ω—É—é")
                {

                    await botClient.SendTextMessageAsync(message.Chat.Id, // –ù–∞–ø–∏—à–µ—Ç –≤ —á–∞—Ç 
                        "ü§ñ–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å.\n" +
                        "ü§ñ–Ø –†–° –±–æ—Ç –ê–Ω–æ–Ω–∏–º–Ω—ã—Ö –Ω–∞—Ä–∫–æ–º–∞–Ω–æ–≤ –°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä—É–±—Ä–≥–∞.\n");

                    ReplyKeyboardMarkup keyboard = new(
                        new[]
                    {
                                new KeyboardButton[] {mainMenu[0]},
                                new KeyboardButton[] {mainMenu[1]},
                                new KeyboardButton[] {mainMenu[2]},
                                new KeyboardButton[] {mainMenu[3]},
                    })
                    {
                        ResizeKeyboard = true
                    };
                    await botClient.SendTextMessageAsync(message.Chat.Id, "ü§ñ–í—ã–±–µ—Ä–∏—Ç–µ –∫–ª–∞–≤–∏—à—É:", replyMarkup: keyboard);
                    return;
                }
                //–Æ–±–∏–ª–µ–π 33 –≥–æ–¥–∞
                if (message.Text == mainMenu[3])
                {
                    await anniversaryAsync(botClient, message);
                    return;
                }
                //anniversaryVakantArray[0]
                if (message.Text == anniversaryVakantArray[0])
                {
                    await anniversaryVakantAsync(botClient, message);
                    return;
                }
                //–ß—Ç–æ —Ç–∞–∫–æ–µ –ê–ù?
                if (message.Text == mainMenu[2])
                {
                    await WhatThisAnAsync(botClient, message);
                    return;
                }
                //–†–∞–∑–≤—è–∑–∫–∞ –ø–æ —Ä–∞—Å–ø–∏—Å–∞–Ω–∏—é –≥—Ä—É–ø–ø –≤
                if (message.Text == mainMenu[1])
                {
                    await PrintSheduleGroupAsync(botClient, message);
                    return;
                }

                //–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –≥—Ä—É–ø–ø –≤ –ü–∏—Ç–µ—Ä–µ
                if (message.Text == sheduleStruct[0])
                {
                    await PrintScheduleGroupInSaintPetersburgAsync(botClient, message);
                    return;
                }
                
                for (int i = 0; i < gruopNamesArray.Length; i++)
                {
                    if(message.Text == gruopNamesArray[i])
                    {
                        await PrintScheduleGroupInSaintPetersburg1Async(botClient, message);
                        return;
                    }
                }

                //–Ω–∞—á–∞–ª–æ –≤–µ—Ç–∫–∏ –ø–æ–¥–∫–æ–º–∏—Ç–µ—Ç–æ–≤
                if (message.Text == mainMenu[0])
                {
                    ReplyKeyboardMarkup keyboard = new(
                        new[]
                            {
                             new KeyboardButton[] {subcommitteesMenu[0]},
                             new KeyboardButton[] {"ü™ú–ù–∞ –≥–ª–∞–≤–Ω—É—é"},

                            })
                    {
                        ResizeKeyboard = true
                    };
                    await botClient.SendTextMessageAsync(message.Chat.Id, "ü§ñ–í—ã–±–µ—Ä–∏—Ç–µ –∫–ª–∞–≤–∏—à—É:", replyMarkup: keyboard);
                    return;
                }

                long someUserChatId = 000000000000;

                //–ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –∫ —Ä–∞—Å—Å—ã–ª–∫–µ
                if (message.Text == rsMenu[2])
                {
                    await botClient.SendTextMessageAsync(message.Chat.Id, // –ù–∞–ø–∏—à–µ—Ç –≤ —á–∞—Ç 
                         "https://t.me/naspb12\n" +
                         "ü§ñ–ü–µ—Ä–µ–π–¥–∏—Ç–µ ‚òùüèª\n");
                    return;
                }
                //–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –†–°
                if (message.Text == rsMenu[1])
                {
                    await PrintInfoAboutRsAsync(botClient, message);
                    return;
                }
                //–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ –ø–æ–¥–∫–æ–º–∏—Ç–µ—Ç–µ                
                if (message.Text == rsMenu[0])
                {
                    await PrintSeminarOfRsAsync(botClient, message);
                    return;
                }
                //–∑–∞–∫–∞–∑–∞—Ç—å —Å–µ–º–∏–Ω–∞—Ä
                for (int i = 0; i < seminarNamesArray.Length; i++)
                {
                    if (message != null && string.Equals(message.Text, seminarNamesArray[i].Replace("\n", "")))
                    {
                        await PrintNamesGroupForSeminarsAsync(botClient, message);

                        userRequest[0] = message.Text;
                        return;
                    }
                }

                /*if (message != null && string.Equals(message.Text, seminarNamesArray[0].Replace("\n", "")))                
                {
                    await PrintNamesGroupForSeminarsAsync(botClient, message);
                    return;
                }*/

                //–†–∞–∑–≤—è–∑–∫–∞ –ü–†–°                
                if (message.Text == subcommitteesMenu[0] || message.Text == subcommitteesMenu[1])
                {
                    await PrintPrsAsync(botClient, message);
                    return;
                }
                //–ó–∞–∫–∞–∑–∞—Ç—å —Å–µ–º–∏–Ω–∞—Ä –Ω–∞ —É–∫–∞–∑–∞–Ω–Ω–æ–µ –≤—Ä–µ–º—è
                for (int i = 0; i < gruopNamesForSeminarsArray.Length; i++)
                {
                    if (message.Text == gruopNamesForSeminarsArray[i])
                    {
                        await timeNamesforSeminarAsync(botClient, message);
                        userRequest[1] = message.Text;
                        return;
                    }
                }
                //–û—Ç–æ—Å–ª–∞—Ç—å —á–µ–ª–æ–≤–µ–∫—É –º–∞—Å—Å–∏–≤ —Å–æ–±—Ä–∞–Ω–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö –ø–æ –∑–∞–∫–∞–∑–∞—É —Å–µ–º–∏–Ω–∞—Ä–∞ (–°–µ–º–∏–Ω–∞—Ä, –ì—Ä—É–ø–ø–∞, –í—Ä–µ–º—è)
                for (int i = 0; i < timeNamesRequest.Length; i++)
                {
                    if (message.Text == timeNamesRequest[i])
                    {
                        userRequest[2] = message.Text;
                        await SendASeminarRequest(botClient, message);
                        return;
                    }
                }

                for (int i = 0; i < workDateSeminarArray.Length; i++)
                {
                    if (message.Text == workDateSeminarArray[i])
                    {
                        userRequest[3] = message.Text;
                        await SendDataSeminarRequest(botClient, message);
                        return;
                    }
                }
                return;
            }
        }

        //–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –†–°
        private static async Task<string> PrintInfoAboutRsAsync(ITelegramBotClient botClient, Message message)
        {

            // –ù–∞–ø–∏—à–µ—Ç –≤ —á–∞—Ç 
            await botClient.SendTextMessageAsync(message.Chat.Id,
            "ü§ñ–ì–ª–∞–≤–Ω—ã–º–∏  —Ü–µ–ª—è–º–∏ –ü–æ–¥–∫–æ–º–∏—Ç–µ—Ç–∞ –ø–æ –†–∞–∑–≤–∏—Ç–∏—é –°–æ–æ–±—â–µ—Å—Ç–≤–∞ —è–≤–ª—è—é—Ç—Å—è:–û–∫–∞–∑–∞–Ω–∏–µ –ø–æ–º–æ—â–∏ –Ω–æ–≤—ã–º –≥—Ä—É–ø–ø–∞–º\n" +
            "ü§ñ–ü–æ–¥–¥–µ—Ä–∂–∫–∞ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö –≥—Ä—É–ø–ø –ê–ù –≤ —É–ª—É—á—à–µ–Ω–∏–∏ —ç—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω–æ—Å—Ç–∏ —Å–ª—É–∂–µ–Ω–∏—è –∏ –Ω–µ—Å–µ–Ω–∏—è –≤–µ—Å—Ç–∏ –ê–ù\n " +
            "ü§ñ–°–æ–¥–µ–π—Å—Ç–≤–æ–≤–∞—Ç—å —Ä–∞–∑–≤–∏—Ç–∏—é –º–µ—Å—Ç–Ω–æ—Å—Ç–∏ –≤ —Ü–µ–ª–æ–º –ú—ã –±–ª–∞–≥–æ–¥–∞—Ä–Ω—ã –∑–∞ —Ç–æ, —á—Ç–æ –∏–º–µ–µ–º, –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å —Å–ª—É–∂–∏—Ç—å –≤ –ê–ù. \n" +
            "ü§ñ–ö–æ–≥–¥–∞-—Ç–æ –º—ã –±–µ—Å–∫–æ—Ä—ã—Å—Ç–Ω–æ –ø–æ–ª—É—á–∏–ª–∏ –¥–∞—Ä –≤—ã–∑–¥–æ—Ä–æ–≤–ª–µ–Ω–∏—è, —Ç–µ–ø–µ—Ä—å —Ç–∞–∫–∂–µ –±–µ—Å–∫–æ—Ä—ã—Å—Ç–Ω–æ –æ—Ç–¥–∞–µ–º.\n");
            //–ø–æ–¥–≥–æ—Ç–æ–≤–∫–∞ –∫–Ω–æ–ø–æ–∫ –Ω–∞ –¥–∏—Å–ø–ª–µ–π
            ReplyKeyboardMarkup keyboard = new(
                new[]
                {
                        new KeyboardButton[] {"‚¨ÖÔ∏è–ü–†–°"},
                })
            {
                ResizeKeyboard = true
            };
            //–í—ã–∫–∞—Ç–∏—Ç—å –∫–Ω–æ–ø–∫–∏ –Ω–∞ –¥–∏—Å–ø–ª–µ–π
            await botClient.SendTextMessageAsync(message.Chat.Id, "ü§ñ–ü–æ–∂–∞–ª—É–π—Å—Ç–∞ –ø–µ—Ä–µ–π–¥–∏—Ç–µ –æ–±—Ä–∞—Ç–Ω–æ", replyMarkup: keyboard);

            return "";
        }
        //–†–∞—Å–ø–∏—Å–∞–Ω–∏–µ –≥—Ä—É–ø–ø
        private static async Task<string> PrintScheduleGroupInSaintPetersburgAsync(ITelegramBotClient botClient, Message message)
        {
            var allGroupNames = "ü§ñC–ø–∏—Å–æ–∫ –≥—Ä—É–ø–ø:\n\n";

            for (int j = 0; j < gruopNamesArray.Length; j++)
            {
                allGroupNames += (gruopNamesArray[j]+"\n");
            }
            await botClient.SendTextMessageAsync(message.Chat.Id, allGroupNames);
            //–ø–æ–¥–≥–æ—Ç–æ–≤–∫–∞ –∫–Ω–æ–ø–æ–∫ –Ω–∞ –¥–∏—Å–ø–ª–µ–π
            ReplyKeyboardMarkup keyboard = new(
                new[]
                {
                    new KeyboardButton[] {"ü™ú–ù–∞ –≥–ª–∞–≤–Ω—É—é"},
                    new KeyboardButton[] {gruopNamesArray[0]},
                    new KeyboardButton[] {gruopNamesArray[1]},
                    new KeyboardButton[] {gruopNamesArray[2]},
                    new KeyboardButton[] {gruopNamesArray[3]},
                    new KeyboardButton[] {gruopNamesArray[4]},
                    new KeyboardButton[] {gruopNamesArray[5]},
                    new KeyboardButton[] {gruopNamesArray[6]},
                    new KeyboardButton[] {gruopNamesArray[7]},
                    new KeyboardButton[] {gruopNamesArray[8]},
                    new KeyboardButton[] {gruopNamesArray[9]},
                    new KeyboardButton[] {gruopNamesArray[10]},
                    new KeyboardButton[] {gruopNamesArray[11]},
                    new KeyboardButton[] {gruopNamesArray[12]},
                    new KeyboardButton[] {gruopNamesArray[13]},
                    new KeyboardButton[] {gruopNamesArray[14]},
                    new KeyboardButton[] {gruopNamesArray[15]},
                    new KeyboardButton[] {"\U0001fa9c–ù–∞ –≥–ª–∞–≤–Ω—É—é"},
                })
            {
                ResizeKeyboard = true
            };
            //–í—ã–∫–∞—Ç–∏—Ç—å –∫–Ω–æ–ø–∫–∏ –Ω–∞ –¥–∏—Å–ø–ª–µ–π
            await botClient.SendTextMessageAsync(message.Chat.Id, "ü§ñ–ü–æ–∂–∞–ª—É–π—Å—Ç–∞ –≤—ã–±–µ—Ä–∏—Ç–µ –Ω—É–∂–Ω—É—é –≤–∞–º –≥—Ä—É–ø–ø—É", replyMarkup: keyboard);
            return "";
        }
        //C–ø–∏—Å–æ–∫ —Å–µ–º–∏–Ω–∞—Ä–æ–≤
        private static async Task<string> PrintSeminarOfRsAsync(ITelegramBotClient botClient, Message message)
        {
            var allSeminarNames = "ü§ñC–ø–∏—Å–æ–∫ —Å–µ–º–∏–Ω–∞—Ä–æ–≤:\n\n";

            for (int i = 0; i < seminarNamesArray.Length; i++)
            {
                allSeminarNames += seminarNamesArray[i];
            }
            await botClient.SendTextMessageAsync(message.Chat.Id, allSeminarNames);

            ReplyKeyboardMarkup keyboard = new(
                new[]
                    {
                        new KeyboardButton[] {"‚¨ÖÔ∏è–ü–†–°"},
                        new KeyboardButton[] {seminarNamesArray[0]},
                        new KeyboardButton[] {seminarNamesArray[1], seminarNamesArray[2]},
                        new KeyboardButton[] {seminarNamesArray[3]},
                        new KeyboardButton[] {seminarNamesArray[4]},
                        new KeyboardButton[] {seminarNamesArray[5]},
                        new KeyboardButton[] {seminarNamesArray[6]},
                        new KeyboardButton[] {seminarNamesArray[7]},
                        new KeyboardButton[] {"‚¨ÖÔ∏è–ü–†–°"},
                    })
            {
                ResizeKeyboard = true
            };

            await botClient.SendTextMessageAsync(message.Chat.Id, "–í—ã –º–æ–∂–µ—Ç–µ –ª–∏—Å—Ç–∞—Ç—å –≤–Ω–∏–∑", replyMarkup: keyboard);
            return "";
        }
        //–ß—Ç–æ —Ç–∞–∫–æ–µ –ê–ù?
        private static async Task<string> WhatThisAnAsync(ITelegramBotClient botClient, Message message)
        {
            await botClient.SendTextMessageAsync(message.Chat.Id, // –ù–∞–ø–∏—à–µ—Ç –≤ —á–∞—Ç 
                        "–ê–Ω–æ–Ω–∏–º–Ω—ã–µ –ù–∞—Ä–∫–æ–º–∞–Ω—ã ‚Äì —ç—Ç–æ —Å–æ–æ–±—â–µ—Å—Ç–≤–æ –º—É–∂—á–∏–Ω –∏ –∂–µ–Ω—â–∏–Ω, –¥–ª—è –∫–æ—Ç–æ—Ä—ã—Ö –Ω–∞—Ä–∫–æ—Ç–∏–∫–∏ —Å—Ç–∞–ª–∏ –≥–ª–∞–≤–Ω–æ–π –ø—Ä–æ–±–ª–µ–º–æ–π. –í—ã–∑–¥–æ—Ä–∞–≤–ª–∏–≤–∞—é—â–∏–µ –∑–∞–≤–∏—Å–∏–º—ã–µ - —Ä–µ–≥—É–ª—è—Ä–Ω–æ —Å–æ–±–∏—Ä–∞—é—Ç—Å—è, —á—Ç–æ–±—ã –ø–æ–º–æ—á—å –¥—Ä—É–≥ –¥—Ä—É–≥—É –≤–æ–∑–¥–µ—Ä–∂–∞—Ç—å—Å—è –æ—Ç –Ω–∞—Ä–∫–æ—Ç–∏–∫–æ–≤. –≠—Ç–∞ –ø—Ä–æ–≥—Ä–∞–º–º–∞ –ø—Ä–µ–¥—É—Å–º–∞—Ç—Ä–∏–≤–∞–µ—Ç –ø–æ–ª–Ω–æ–µ –≤–æ–∑–¥–µ—Ä–∂–∞–Ω–∏–µ –æ—Ç –≤—Å–µ—Ö –≤–∏–¥–æ–≤ —Ö–∏–º–∏—á–µ—Å–∫–∏—Ö –≤–µ—â–µ—Å—Ç–≤, –∏–∑–º–µ–Ω—è—é—â–∏—Ö —Å–æ–∑–Ω–∞–Ω–∏–µ. –ï–¥–∏–Ω—Å—Ç–≤–µ–Ω–Ω—ã–º —É—Å–ª–æ–≤–∏–µ–º –¥–ª—è –≤—Å—Ç—É–ø–ª–µ–Ω–∏–µ –≤ —Ä—è–¥—ã –ê–ù ‚Äì —è–≤–ª—è–µ—Ç—Å—è –∂–µ–ª–∞–Ω–∏–µ –±—Ä–æ—Å–∏—Ç—å –ø—Ä–∏–Ω–∏–º–∞—Ç—å –Ω–∞—Ä–∫–æ—Ç–∏–∫–∏. –ê–ù –Ω–µ –¥–∞–µ—Ç —á–ª–µ–Ω–∞–º –æ–±—è–∑–∞—Ç–µ–ª—å–Ω—ã—Ö –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏–π, –Ω–æ –ø—Ä–µ–¥–ª–∞–≥–∞–µ—Ç –æ—Ç–∫–∞–∑–∞—Ç—å—Å—è –æ—Ç –ø—Ä–µ–¥—É–±–µ–∂–¥–µ–Ω–∏–π –∏ –¥–∞—Ç—å —Å–µ–±–µ —à–∞–Ω—Å. –≠—Ç–∞ –ø—Ä–æ–≥—Ä–∞–º–º–∞ –æ—Å–Ω–æ–≤–∞–Ω–∞ –Ω–∞ –ø—Ä–∏–Ω—Ü–∏–ø–∞—Ö, –∏–∑–ª–æ–∂–µ–Ω–Ω—ã—Ö —Ç–∞–∫ –ø—Ä–æ—Å—Ç–æ, —á—Ç–æ –∏–º –º–æ–∂–Ω–æ —Å–ª–µ–¥–æ–≤–∞—Ç—å –≤ –ø–æ–≤—Å–µ–¥–Ω–µ–≤–Ω–æ–π –∂–∏–∑–Ω–∏. –°–∞–º–æ–µ –≥–ª–∞–≤–Ω–æ–µ –≤ –Ω–∏—Ö —Ç–æ, —á—Ç–æ –æ–Ω–∏ —Ä–∞–±–æ—Ç–∞—é—Ç. –ê–ù –Ω–µ —Å–≤—è–∑–∞–Ω—ã –Ω–∏ —Å –∫–∞–∫–∏–º–∏ –¥—Ä—É–≥–∏–º–∏ –æ—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—è–º–∏. –ù–µ—Ç –≤—Å—Ç—É–ø–∏—Ç–µ–ª—å–Ω—ã—Ö –∏–ª–∏ —á–ª–µ–Ω—Å–∫–∏—Ö –≤–∑–Ω–æ—Å–æ–≤, —É—á–∞—Å—Ç–Ω–∏–∫–∏ –Ω–µ –æ–±—è–∑–∞–Ω—ã –ø–æ–¥–ø–∏—Å—ã–≤–∞—Ç—å –æ–±–µ—Ç—ã –∏–ª–∏ –¥–∞–≤–∞—Ç—å –∫–æ–º—É-–ª–∏–±–æ —É—Å—Ç–Ω—ã–µ –æ–±–µ—â–∞–Ω–∏—è. –ê–ù –Ω–µ —Å–≤—è–∑–∞–Ω—ã –Ω–∏ —Å –∫–∞–∫–∏–º–∏ –ø–æ–ª–∏—Ç–∏—á–µ—Å–∫–∏–º–∏, —Ä–µ–ª–∏–≥–∏–æ–∑–Ω—ã–º–∏, –ø—Ä–∞–≤–æ–æ—Ö—Ä–∞–Ω–∏—Ç–µ–ª—å–Ω—ã–º–∏ –∏–ª–∏ –∫–æ–º–º–µ—Ä—á–µ—Å–∫–∏–º–∏ —Å—Ç—Ä—É–∫—Ç—É—Ä–∞–º–∏ –∏ –Ω–∏–∫—Ç–æ –Ω–µ –∫–æ–Ω—Ç—Ä–æ–ª–∏—Ä—É–µ—Ç –¥–µ—è—Ç–µ–ª—å–Ω–æ—Å—Ç—å —Å–æ–æ–±—â–µ—Å—Ç–≤–∞. –≠—Ç–∏–º–∏ —Å–ª–æ–≤–∞–º–∏ –°–æ–æ–±—â–µ—Å—Ç–≤–æ –≤—Å—Ç—Ä–µ—á–∞–µ—Ç —Å–≤–æ–∏—Ö —É—á–∞—Å—Ç–Ω–∏–∫–æ–≤ —É–∂–µ –±–æ–ª–µ–µ 50 –ª–µ—Ç.\n\n" +
                        "–ü—Ä–æ–≥—Ä–∞–º–º–∞ –≤—ã–∑–¥–æ—Ä–æ–≤–ª–µ–Ω–∏—è –ê–Ω–æ–Ω–∏–º–Ω—ã—Ö –ù–∞—Ä–∫–æ–º–∞–Ω–æ–≤ —ç—Ç–æ –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç—å –ª–∏—á–Ω—ã—Ö –ø–æ—Å—Ç—É–ø–∫–æ–≤, –∏–∑–≤–µ—Å—Ç–Ω–∞—è –∫–∞–∫ ¬´–î–≤–µ–Ω–∞–¥—Ü–∞—Ç—å –®–∞–≥–æ–≤¬ª –≤–∫–ª—é—á–∞–µ—Ç –ø—Ä–∏–∑–Ω–∞–Ω–∏–µ –∏–º–µ—é—â–µ–π—Å—è –ø—Ä–æ–±–ª–µ–º—ã, –æ–±—Ä–∞—â–µ–Ω–∏–µ –∑–∞ –ø–æ–º–æ—â—å—é, –ø—Ä–∞–≤–¥–∏–≤—É—é —Å–∞–º–æ–æ—Ü–µ–Ω–∫—É, –¥–æ–≤–µ—Ä–∏—Ç–µ–ª—å–Ω—ã–π —Ä–∞—Å—Å–∫–∞–∑ –æ —Å–µ–±–µ, –≤–æ–∑–º–µ—â–µ–Ω–∏–µ –ø—Ä–∏—á–∏–Ω–µ–Ω–Ω–æ–≥–æ –æ–∫—Ä—É–∂–∞—é—â–∏–º —É—â–µ—Ä–±–∞ –∏ –ø–æ–º–æ—â—å –∂–µ–ª–∞—é—â–∏–º –≤—ã–∑–¥–æ—Ä–æ–≤–µ—Ç—å –Ω–∞—Ä–∫–æ–º–∞–Ω–∞–º. –í–∞–∂–Ω–µ–µ –≤—Å–µ–≥–æ –≤ –ø—Ä–æ–≥—Ä–∞–º–º–µ ‚Äì –∞–∫—Ü–µ–Ω—Ç –Ω–∞ —Ç–æ–º, —á—Ç–æ –æ–±–æ–∑–Ω–∞—á–∞–µ—Ç—Å—è –∫–∞–∫ ¬´–¥—É—Ö–æ–≤–Ω–æ–µ –ø—Ä–æ–±—É–∂–¥–µ–Ω–∏–µ¬ª.\n\n" +
                        "–ü–µ—Ä–≤–æ–µ, —á—Ç–æ –æ–±–µ—Å–ø–µ—á–∏–≤–∞–µ—Ç –ê–Ω–æ–Ω–∏–º–Ω—ã–µ –ù–∞—Ä–∫–æ–º–∞–Ω—ã —ç—Ç–æ —Å–æ–±—Ä–∞–Ω–∏—è –≥—Ä—É–ø–ø –ê–ù.–ö–∞–∂–¥–∞—è –≥—Ä—É–ø–ø–∞ —Å–∞–º–æ—É–ø—Ä–∞–≤–ª—è–µ–º–∞—è, —Ä—É–∫–æ–≤–æ–¥—Å—Ç–≤—É—è—Å—å –ø—Ä–∏–Ω—Ü–∏–ø–∞–º–∏(—Ç—Ä–∞–¥–∏—Ü–∏—è–º–∏), –æ–±—â–∏–º–∏ –¥–ª—è –≤—Å–µ–≥–æ –°–æ–æ–±—â–µ—Å—Ç–≤–∞, –∏ –æ–ø–∏—Å–∞–Ω–Ω—ã–º–∏ –≤ –Ω–∞—à–∏—Ö –∫–Ω–∏–≥–∞—Ö.–°–æ–±—Ä–∞–Ω–∏—è –º–æ–≥—É—Ç –±—ã—Ç—å ¬´–æ—Ç–∫—Ä—ã—Ç—ã–º–∏¬ª, –≥–¥–µ –º–æ–∂–µ—Ç –ø—Ä–∏—Å—É—Ç—Å—Ç–≤–æ–≤–∞—Ç—å –ª—é–±–æ–π —á–µ–ª–æ–≤–µ–∫, –∏–ª–∏ ¬´–∑–∞–∫—Ä—ã—Ç—ã–º–∏¬ª, –∫—É–¥–∞ –¥–æ–ø—É—Å–∫–∞—é—Ç—Å—è –ª–∏—à—å —Ç–µ, –∫—Ç–æ —Å–∞–º –∏–º–µ–µ—Ç –ø—Ä–æ–±–ª–µ–º—É —Å –Ω–∞—Ä–∫–æ—Ç–∏–∫–∞–º–∏.\n\n" +
