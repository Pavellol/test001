        //–æ—Ç–ø—Ä–∞–≤–∏—Ç—å –∑–∞–ø—Ä–æ—Å –∞–¥–º–∏–Ω—É
        private static async Task<string> SendDataSeminarRequest(ITelegramBotClient botClient, Message message)
        {
            string userRequestSeminar = "\n";

            for (int i = 0; i < userRequest.Length; i++)
            {
                userRequestSeminar += (userRequest[i]+ Environment.NewLine);
            }

            ReplyKeyboardMarkup keyboard = new(
                new[]
                {
                    new KeyboardButton[] { rsMenu[0]},
                })
            {
                ResizeKeyboard = true
            };

            await botClient.SendTextMessageAsync(message.Chat.Id, "ü§ñ–•–æ—Ä–æ—à–æ, –¥–ª—è –¥–∞–ª—å–Ω–µ–π—à–µ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ —Å –≤–∞–º–∏ —Å–≤—è–∂—É—Ç—Å—è –≤ —Ç–µ—á–µ–Ω–∏–∏ 72-—É—Ö —á–∞—Å–æ–≤",
                replyMarkup: keyboard);

            await botClient.SendTextMessageAsync(-842563538, userRequestSeminar);
            await botClient.ForwardMessageAsync(-842563538, message.Chat.Id, message.MessageId);
            return "";
        }
