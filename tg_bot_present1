            ReplyKeyboardMarkup keyboard = new(
                new[]
                {                    
                    new KeyboardButton[] { anniversaryVakantArray[0] },
                    new KeyboardButton[] { "ü™ú–ù–∞ –≥–ª–∞–≤–Ω—É—é"},
                })
            {
                ResizeKeyboard = true
            };

            await botClient.SendTextMessageAsync(message.Chat.Id, "ü§ñ–ú–µ—Ç–æ –ø—Ä–æ–≤–µ–¥–µ–Ω–∏—è: –°–µ–≤–∫–∞–±–µ–ª—å –ø–æ—Ä—Ç.\n" +
                "–î–∞—Ç–∞ –ø—Ä–æ–≤–µ–¥–µ–Ω–∏—è: –Ω–∞—á–∞–ª–æ –º–∞—è\n" +
                "–°–≤–æ–±–æ–¥–Ω—ã–π –≤—Ö–æ–¥, —Ñ—É–¥–∫–æ—Ä—Ç—ã, –¥–∏—Å–∫–æ—Ç–µ–∫–∞!\n" +
                "\"–ú—ã —É–∂–µ –∑–∞–ø–ª–∞—Ç–∏–ª–∏,—á—Ç–æ –±—ã –±—ã—Ç—å –∑–¥–µ—Å—å\"",
                replyMarkup: keyboard);
            return "";
        }

